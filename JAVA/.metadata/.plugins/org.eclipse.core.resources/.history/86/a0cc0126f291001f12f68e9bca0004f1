package Day2;
import java.util.Scanner;
public class arraySort{

	public static void main(String[] args) {
		int a[] = new int [5];
		Scanner sc =new Scanner(System.in);
		for (int i=0 ; i<5;i++ ) {
			System.out.print("Enter the element : ");
			a[i]=sc.nextInt();
		}
		for(int i:a) {
			System.out.println(i);
		}
	}

}
