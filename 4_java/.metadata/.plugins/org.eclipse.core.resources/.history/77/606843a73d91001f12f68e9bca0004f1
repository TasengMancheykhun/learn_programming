package Day1;

import java.util.Scanner;

public class pattern2_8 {

	public static void main(String[] args) {
        int num, rem=0;
        
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a number: ");
        num = sc.nextInt();
        
        int count=1;
        while(num>0)
        {
        	while(count>0)
        	{
        	    System.out.println(num);
        	}
        	count+=1;
        	
        }
        
	}

}
